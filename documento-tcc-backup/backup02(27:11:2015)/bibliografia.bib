@article{Crespo,
    abstract = {Abstract Smoothed Particle Hydrodynamics (SPH) is the most widely established mesh-free method which has been used in several fields as astrophysics, solids mechanics and fluid dynamics. In the particular case of computational fluid dynamics, the model is beginning to reach a maturity that allows carrying out detailed quantitative comparisons with laboratory experiments. Here the state-of-the-art of the classical SPH formulation for free-surface flow problems is described in detail. This is demonstrated using dam-break simulations in 2-D and 3-D. The foundations of the method will be presented using different derivations based on the method of interpolants and on the moving least-squares approach. Different methods to improve the classic SPH approach such as the use of density filters and the corrections of the kernel function and its gradient are examined and tested on some laboratory cases. 2010 International Association of Hydraulic Engineering and Research.},
    author = {Gomez-Gesteira, M. and Rogers, B. D. and Dalrymple, R. A. and Crespo, A. J. C.},
    citeulike-article-id = {9420483},
    citeulike-linkout-0 = {http://dx.doi.org/10.1080/00221686.2010.9641242},
    citeulike-linkout-1 = {http://dx.doi.org/10.1080/00221686.2010.9641242},
    doi = {10.1080/00221686.2010.9641242},
    journal = {Journal of Hydraulic Research},
    keywords = {cfd, meshfree, sph, state-of-the-art},
    number = {S1},
    pages = {6--27},
    posted-at = {2011-06-16 10:08:43},
    priority = {5},
    publisher = {Taylor \& Francis},
    title = {State-of-the-art of classical SPH for free-surface flows},
    url = {http://dx.doi.org/10.1080/00221686.2010.9641242},
    Urlaccessdate = {01.2014},
    volume = {48},
    year = {2010}
}

